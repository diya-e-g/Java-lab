import java.io.*;

public class CopyFile
{
    public static void main(String args[]) throws Exception
    {
        FileInputStream fis=new FileInputStream("in.txt");
        FileOutputStream fos=new FileOutputStream("out.txt");
        int b;
	while((b=fis.read())!=-1)
	{
		fos.write(b);
	}

       
        
    }
}
